!/bin/bash -x
#FLIP=$(($(($RANDOM%10)%2))
#if [ $FLIP -eq 1 ];then
 #   echo "heads"
#else
 #   echo "tails"
#fi
Result=$((RANDOM%2))
if [[ ${Result} -eq 0 ]]; then
    echo HEADS
elif [[ ${Result} -eq 1 ]]; then
    echo TAILS
fi
echo "Result" ${#Result[HEADS]}
echo "Result" ${#Result[TAILS]}
echo "Number of TAILS" ${#Result[@]}
echo "Number of Heads" ${#Result[@]}




